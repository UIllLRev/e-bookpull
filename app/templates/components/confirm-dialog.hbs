{{#mdc-dialog title=title}}
  {{message}}
  <footer class="mdc-dialog__actions">
    {{#mdc-dialog-button style="raised" click=(action "yes")}} {{yes_message}} {{/mdc-dialog-button}}
    {{#mdc-dialog-button style="raised" isAccent=true click=(action "no")}} {{no_message}} {{/mdc-dialog-button}}
  </footer>
{{/mdc-dialog}}
